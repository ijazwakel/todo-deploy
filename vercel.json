{
    "builds": [
      {
        "src": "index.js",  
        "use": "@vercel/node"
      },
         {
        "src": "dist/",  
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/todo/(.*)",
        "dest": "index.js" 
      },
         {
        "src": "/",
        "dest": "dist/index.html" 
      }
    ]
  }
  
