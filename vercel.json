{
    "builds": [
      {
        "src": "server/index.js",  
        "use": "@vercel/node"
      },
         {
        "src": "dist/",  
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/todo/(.*)",
        "dest": "server/index.js" 
      },
         {
        "src": "/(.*)",
        "dest": "dist/" 
      }
    ]
  }
  
